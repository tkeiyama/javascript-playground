{
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    ".react-router/types/**/*"
  ],
  "compilerOptions": {
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "noEmit": true,
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "module": "esnext",
    "skipLibCheck": true
  }
}
